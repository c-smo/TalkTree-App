{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "websocket:default",
    "sql:default",
    "websocket:allow-send",
    "websocket:allow-connect",
    "sql:allow-close",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "tts:allow-speak"
  ]
}